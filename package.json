  },
  "bugs": {
    "email": "direflow@silind.com",
    "url": "https://github.com/Silind-Software/direflow/issues/new"
  },
  "main": "build/direflowBundle.js",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rm -rf build dist lib",
